{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "pycon",
  "scopeName": "text.python.console",
  "patterns": [
    {
      "match": "^(>{3}|\\.{3}|In \\[\\d+\\]:) (.+)$",
      "captures": {
        "1": {
          "name": "punctuation.separator.prompt.python.console"
        },
        "2": {
          "patterns": [
            {
              "include": "source.python"
            }
          ]
        }
      }
    }
  ]
}
