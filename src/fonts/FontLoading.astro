---
import { SITE } from "#src/config.ts";
import "#src/fonts/index.ts";

// NOTE: Attribute crossorigin="anonymouse" is always required
//       even if the font is hosted on the same domain
---

<link
  rel="preload"
  href={SITE.theme.mainFont.path}
  as="font"
  type={SITE.theme.mainFont.linkType}
  crossorigin="anonymous"
/>
